{
	"name": "tabzero",
	"author": "Haydn Comley",
	"description": "AI Stream Assistant - Never tab out again.",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/haydncomley/tabzero.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build && electron-builder",
		"lint": "eslint . --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"format": "prettier --write ./src",
		"api:deploy": "firebase deploy --only=functions",
		"api:emulate": "FIRESTORE_EMULATOR_HOST=localhost:8080 firebase emulators:start",
		"app:start": "electron-forge start",
		"app:package": "electron-forge package",
		"app:make": "electron-forge make",
		"app:publish": "electron-forge publish"
	},
	"dependencies": {
		"@stripe/stripe-js": "7.3.0",
		"@tanstack/react-query": "^5.76.1",
		"classnames": "^2.5.1",
		"electron-squirrel-startup": "1.0.1",
		"firebase": "^11.7.3",
		"lucide-react": "^0.511.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router": "7.6.0",
		"react-router-dom": "7.6.0",
		"update-electron-app": "3.1.1",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"devDependencies": {
		"@electron-forge/cli": "7.8.1",
		"@electron-forge/maker-deb": "7.8.1",
		"@electron-forge/maker-rpm": "7.8.1",
		"@electron-forge/maker-squirrel": "7.8.1",
		"@electron-forge/maker-zip": "7.8.1",
		"@electron-forge/plugin-auto-unpack-natives": "7.8.1",
		"@electron-forge/plugin-fuses": "7.8.1",
		"@electron-forge/publisher-github": "7.8.1",
		"@electron/fuses": "1.8.0",
		"@tailwindcss/vite": "4.1.7",
		"@types/react": "^18.2.64",
		"@types/react-dom": "^18.2.21",
		"@typescript-eslint/eslint-plugin": "8.32.1",
		"@typescript-eslint/parser": "8.32.1",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "10.4.21",
		"electron": "^30.0.1",
		"eslint": "^8.57.0",
		"eslint-import-resolver-typescript": "^4.3.5",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-prettier": "5.4.0",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"postcss": "8.5.3",
		"prettier": "3.5.3",
		"prettier-plugin-tailwindcss": "0.6.11",
		"tailwindcss": "4.1.7",
		"typescript": "^5.2.2",
		"vite": "^5.1.6",
		"vite-plugin-electron": "^0.28.6",
		"vite-plugin-electron-renderer": "^0.14.5",
		"vite-plugin-pages": "0.33.0"
	},
	"main": "dist-electron/main.js"
}
